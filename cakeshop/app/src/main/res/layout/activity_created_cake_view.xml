<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/canvas"
    tools:context=".CreatedCakeViewer"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tviewCakeName"
        android:layout_width="107dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="Cake name:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.095"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127" />

    <TextView
        android:id="@+id/cakeName"
        android:layout_width="127dp"
        android:layout_height="29dp"
        android:text="Unknown"
        android:textColor="#000000"
        android:gravity="center"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.297"
        app:layout_constraintStart_toEndOf="@+id/tviewCakeName"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.128" />

    <TextView
        android:id="@+id/tviewCakeShape"
        android:layout_width="111dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="Cake shape:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.096"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tviewCakeName"
        app:layout_constraintVertical_bias="0.065" />


    <TextView
        android:id="@+id/cakeShape"
        android:layout_width="127dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="Unknown"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.277"
        app:layout_constraintStart_toEndOf="@+id/tviewCakeShape"
        app:layout_constraintTop_toBottomOf="@+id/cakeName"
        app:layout_constraintVertical_bias="0.065" />

    <TextView
        android:id="@+id/tviewCakeFlavour"
        android:layout_width="119dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="Cake flavour:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.099"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tviewCakeShape"
        app:layout_constraintVertical_bias="0.074" />

    <TextView
        android:id="@+id/cakeFlavour"
        android:layout_width="127dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="Unknown"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.235"
        app:layout_constraintStart_toEndOf="@+id/tviewCakeFlavour"
        app:layout_constraintTop_toBottomOf="@+id/cakeName"
        app:layout_constraintVertical_bias="0.186" />

    <TextView
        android:id="@+id/tviewCakeSize"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:gravity="center"
        android:text="Cake size:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tviewCakeFlavour"
        app:layout_constraintVertical_bias="0.073" />

    <TextView
        android:id="@+id/cakeSize"
        android:layout_width="127dp"
        android:layout_height="29dp"
        android:gravity="center"
        android:text="Unknown"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.329"
        app:layout_constraintStart_toEndOf="@+id/tviewCakeSize"
        app:layout_constraintTop_toBottomOf="@+id/cakeName"
        app:layout_constraintVertical_bias="0.3" />

    <TextView
        android:id="@+id/tviewCakeDescription"
        android:layout_width="157dp"
        android:layout_height="33dp"
        android:layout_marginBottom="252dp"
        android:gravity="center"
        android:text="Cake description:"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tviewCakeSize"
        app:layout_constraintVertical_bias="0.227" />

    <TextView
        android:id="@+id/cakeDescription"
        android:layout_width="304dp"
        android:layout_height="122dp"
        android:text="Unknown"
        android:textColor="#000000"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.271"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tviewCakeDescription"
        app:layout_constraintVertical_bias="0.109" />

</androidx.constraintlayout.widget.ConstraintLayout>